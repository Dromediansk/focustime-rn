{
  "expo": {
    "name": "Focustime",
    "slug": "focustime",
    "scheme": "focustime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/focustime.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003545"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dromediansk.focustime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/focustime.png",
        "backgroundColor": "#003545"
      },
      "package": "com.dromediansk.focustime"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/play/Play_400Regular.ttf",
            "node_modules/@expo-google-fonts/play/Play_700Bold.ttf",
            "node_modules/@expo-google-fonts/syne-mono/SyneMono_400Regular.ttf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/focustime.png",
          "color": "#003545",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2d686616-fb66-4821-bb07-f3ee8c31e9fb"
      }
    },
    "owner": "dromediansk"
  }
}
