{
  "expo": {
    "name": "Focustime",
    "slug": "focustime",
    "scheme": "focustime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/focustime.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003545"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dromediansk.focustime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/focustime.png",
        "backgroundColor": "#003545"
      },
      "package": "com.dromediansk.focustime"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/play/Play_400Regular.ttf"]
        }
      ]
    ]
  }
}
